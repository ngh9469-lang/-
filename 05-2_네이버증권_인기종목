   "source": [
    "# top 10 가격 수집하고 리스트로 저장\n",
    "price_list = []\n",
    "\n",
    "for i in range(1, 11):\n",
    "    selector = f\"#popularItemList > li:nth-child({i}) > span\"\n",
    "    price_raw = soup.select_one(selector).text\n",
    "    \n",
    "    if ',' in price_raw:   # 가격에 콤마(,)가 있는지 확인하고 있을 때 제거.\n",
    "        price = int(price_raw.replace(\",\", \"\"))\n",
    "    else:\n",
    "        price = int(price_raw)\n",
    "        \n",
    "    price_list.append(price)\n",
    "    # print(price)\n",
    "\n",
    "price_list"
   ]
  },
