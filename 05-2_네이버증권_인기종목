       "<script src=\"https://ssl.pstatic.net/imgstock/static.pc/20260121155431/js/stock.js\" type=\"text/javascript\"></script>\n",
       "<script type=\"text/javascript\">\n",
       "var itemcodes = '086520,005930,000660,005380,196170,035420,247540,090710,034020,068270';\n",
       "var itemInfo = jindo.$H({\"086520\":\"에코프로\",\"005930\":\"삼성전자\",\"000660\":\"SK하이닉스\",\"005380\":\"현대차\",\"196170\":\"알테오젠\",\"035420\":\"NAVER\",\"247540\":\"에코프로비..\",\"090710\":\"휴림로봇\",\"034020\":\"두산에너빌..\",\"068270\":\"셀트리온\"});\n",
       "var pollingInterval = 2000;\n",
       "var pollingAjax = null;\n",
       "var timer = null;\n",
       "\n",
       "function quoteIndexNumberFont(value) {\n",
       "\tvalue = value + \"\";\n",
       "\tvar result = \"\";\n",
       "\n",
       "\tfor (i = 0; i < value.length; i++) {\n",
       "\t\tvar tmpChar = value.charAt(i);\n",
       "\n",
       "\t\tresult += '<span class=\"';\n",
       "\t\tif (tmpChar == \".\") {\n",
       "\t\t\tresult += \"ndot\";\n",
       "\t\t} else if (tmpChar == \",\") {\n",
       "\t\t\tresult += \"ncom\";\n",
       "\t\t} else {\n",
       "\t\t\tresult += \"n\" + tmpChar;\n",
       "\t\t}\n",
       "\t\tresult += '\"><em class=\"blind\">' + tmpChar + '</em></span>';\n",
       "\t}\n",
       "\n",
       "\treturn result;\n",
       "}\n",
       "\n",
       "function formatIndex(status, changeValue, changeRate) {\n",
       "\tvar result = \"\";\n",
       "\n",
       "\tresult = '<span class=\"';\n",
       "\tif (status > 0) {\n",
       "\t\tresult += 'nup\"></span>';\n",
       "\t} else if (status< 0){\n",
       "\t\tresult += 'ndown\"></span>';\n",
       "\t} else {\n",
       "\t\tresult += '\"></span>';\n",
       "\t}\n",
       "\n",
       "\tresult += changeValue;\n",
       "\tresult += ' ';\n",
       "\n",
       "\tif (status > 0) {\n",
       "\t\tresult += '+';\n",
       "\t} else if (status < 0){\n",
       "\t\tresult += '-';\n",
       "\t} else {\n",
       "\t\tresult += '';\n",
       "\t}\n",
       "\n",
       "\tresult += changeRate;\n",
       "\tresult += '%';\n",
       "\n",
       "\tif (status > 0) {\n",
       "\t\tresult += '<span class=\"blind\">상승</span>';\n",
       "\t} else if (status < 0){\n",
       "\t\tresult += '<span class=\"blind\">하락</span>';\n",
       "\t} else {\n",
       "\t\tresult += '<span class=\"blind\">보합</span>';\n",
       "\t}\n",
       "\n",
       "\treturn result;\n",
       "}\n",
       "\n",
       "function displayIndex(id, changeVal, changeRate, nowVal) {\n",
       "\tif (changeVal > 0) {\n",
       "\t\tdocument.getElementById(id + \"_now\").className = \"num\";\n",
       "\t\tdocument.getElementById(id + \"_now\").innerHTML = changeNumberFormat(nowVal.toFixed(2));\n",
       "\n",
       "\t\tdocument.getElementById(id + \"_change\").className = \"num_s\";\n",
       "\t\tdocument.getElementById(id + \"_change\").innerHTML = formatIndex(1, changeNumberFormat(changeVal.toFixed(2)), changeNumberFormat(changeRate.toFixed(2)));\n",
       "\t} else if (changeVal < 0) {\n",
       "\t\tdocument.getElementById(id + \"_now\").className = \"num num2\";\n",
       "\t\tdocument.getElementById(id + \"_now\").innerHTML = changeNumberFormat(nowVal.toFixed(2));\n",
       "\n",
       "\t\tdocument.getElementById(id + \"_change\").className = \"num_s num_s2\";\n",
       "\t\tdocument.getElementById(id + \"_change\").innerHTML = formatIndex(-1, changeNumberFormat((changeVal * -1).toFixed(2)), changeNumberFormat((changeRate * -1).toFixed(2)));\n",
       "\t} else {\n",
       "\t\tdocument.getElementById(id + \"_now\").className = \"num num3\";\n",
       "\t\tdocument.getElementById(id + \"_now\").innerHTML = changeNumberFormat(nowVal.toFixed(2));\n",
       "\n",
       "\t\tdocument.getElementById(id + \"_change\").className = \"num_s num_s3\";\n",
       "\t\tdocument.getElementById(id + \"_change\").innerHTML = formatIndex(0, changeNumberFormat(changeVal.toFixed(2)), changeNumberFormat(changeRate.toFixed(2)));\n",
       "\t}\n",
       "}\n",
       "\n",
       "var startTimer = function() {\n",
       "\tclearTimer();\n",
       "\ttimer = setInterval('doPolling()', pollingInterval);\n",
       "}\n",
       "\n",
       "var clearTimer = function() {\n",
       "\tif (pollingAjax) {\n",
       "\t\tpollingAjax.abort();\n",
       "\t\tpollingAjax = null;\n",
       "\t}\n",
       "\n",
       "\ttimer = window.clearInterval(timer);\n",
       "}\n",
       "\n",
       "var doPolling = function() {\n",
       "\tclearTimer();\n",
       "\tvar pollingApiUrl = 'https://polling.finance.naver.com/api/realtime?query=SERVICE_INDEX:KOSPI,KOSDAQ,KPI200|SERVICE_POPULAR_ITEM:' + itemcodes;\n",
       "\tpollingAjax = jindo.$Ajax(pollingApiUrl, {\n",
       "\t\ttype : 'jsonp',\n",
       "\t\tjsonp_charset : \"euc-kr\",\n",
       "\t\tonload : function(response) {\n",
       "\t\t\tif(response != null && response.readyState() == 4) {\n",
       "\t\t\t\trefreshQuote(response.json());\n",
       "\t\t\t\tstartTimer();\n",
       "\t\t\t}\n",
       "\t\t},\n",
       "\t\ttimeout : 2,\n",
       "\t\tontimeout : function() {\n",
       "\t\t\tstartTimer();\n",
       "\t\t},\n",
       "\t\tasync : true\n",
       "\t});\n",
       "\n",
       "\tpollingAjax.request();\n",
       "}\n",
       "\n",
       "function displayTime(ms, aq, time) {\n",
       "\tvar result;\n",
       "\n",
       "\tvar utcDate = new Date(time);\n",
       "\tutcDate.setMinutes(utcDate.getMinutes() + utcDate.getTimezoneOffset());\n",
       "\tutcDate.setHours(utcDate.getHours() + 9);\n",
       "\n",
       "\tvar oDate = jindo.$Date(utcDate);\n",
       "\n",
       "\tif (ms == \"PREOPEN\") {\n",
       "\t\tif (aq == \"0\") {\n",
       "\t\t\tcloseDate = oDate.format('Y.m.d');\n",
       "\t\t\tresult = oDate.format('Y.m.d') + \"<span>개장전</span></span> \";\n",
       "\t\t} else {\n",
       "\t\t\tcloseDate = oDate.format('Y.m.d');\n",
       "\t\t\tresult = oDate.format('Y.m.d') + \"<span class='sub'>개장전</span> <span class='sub2'>예상지수</span></span> \";\n",
       "\t\t}\n",
       "\t} else if (ms == \"CLOSE\") {\n",
       "\t\tresult = closeDate + \"<span>장마감</span></span> \";\n",
       "\t} else {\n",
       "\t\tcloseDate = oDate.format('Y.m.d');\n",
       "\t\tresult = oDate.format('Y.m.d H:i') + \"<span>장중</span></span>\";\n",
       "\t}\n",
       "\n",
       "\treturn result;\n",
       "}\n",
       "\n",
       "function refreshQuote(res) {\n",
       "\tif (res != null && res.resultCode == 'success') {\n",
